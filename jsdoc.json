{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": [
      "./controllers",
      "./models",
      "./routes",
      "./middlewares",
      "./utils",
      "./config",
      "./app.js",
      "./server.js",
      "./createAdmin.js"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs|out|test)"
  },
  "plugins": [],
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "encoding": "utf8"
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
} 